@model AdminRegisterUpdateDTO
<div class="card col-md-8 offset-2 mt-5">
        <div class="card-body">
            <div class="card-text">
                <h3>Admin Bilgileri</h3>
            </div>
            <form>
                <div class="form-group">
                    <label asp-for="FirstName"></label>
                    <input type="text" class="form-control" asp-for="FirstName" disabled="disabled" />
                </div>
                <div class="form-group">
                    <label asp-for="LastName"></label>
                    <input type="text" class="form-control" asp-for="LastName" disabled="disabled" />
                </div>
                <div class="form-group">
                    <label asp-for="UserName"></label>
                    <input type="text" class="form-control" asp-for="UserName" disabled="disabled" />
                </div>
                <div class="form-group">
                    <label asp-for="Password"></label>
                    <input type="password" class="form-control" value="@Model.Password" disabled="disabled" name="Password" autocomplete="off" />
                </div>
                <div class="form-group">
                    <label asp-for="Email"></label>
                    <input type="text" class="form-control" asp-for="Email" disabled="disabled" autocomplete="off" />
                </div> 
            </form>
        </div>
    </div> 